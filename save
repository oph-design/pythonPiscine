export DOCKER=linux
alias dbuild="docker build -t $DOCKER ~/docker/ && docker run --name $DOCKER -it -v ~/Documents:/host/ $DOCKER zsh"
alias dclean="docker rm $DOCKER && docker rmi $DOCKER"
alias dopen="open -a Docker"
alias dstart="docker start $DOCKER"
alias drun="docker exec -it $DOCKER /bin/zsh"
alias dkill="docker kill $DOCKER"
alias dlink="~/setup.sh"
